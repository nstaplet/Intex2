                    <div style="height:20em;overflow:auto;" class="form-group">
                        <p style="color:red"><b>@ViewBag.NeedEssentialFields</b></p>

                        <input type="hidden" asp-for="BurialId" value="@Model.BurialId" />
                        <input type="hidden" asp-for="LocationId" value="@Model.LocationId" />
                        <input type="hidden" asp-for="SublocationId" value="@Model.SublocationId" />
                        <input type="hidden" asp-for="BurialNumber" value="@Model.BurialNumber" />

                        Measurements (Enter in Meters)
                        <br /><br />
                        <label asp-for="LengthOfRemains">Length of Remains: </label>
                        <input asp-for="LengthOfRemains" value="@Model.LengthOfRemains" />
                        <br />
                        <label asp-for="BurialDepthMeters">Burial Depth: </label>
                        <input asp-for="BurialDepthMeters" value="@Model.BurialDepthMeters" />
                        <br />
                        <label asp-for="SouthToHead">South to Head: </label>
                        <input asp-for="SouthToHead" value="@Model.SouthToHead" />
                        <br />
                        <label asp-for="SouthToFeet">South to Feet: </label>
                        <input asp-for="SouthToFeet" value="@Model.SouthToFeet" />
                        <br />
                        <label asp-for="WestToHead">West to Head: </label>
                        <input asp-for="WestToHead" value="@Model.WestToHead" />
                        <br />
                        <label asp-for="WestToFeet">West to Feet: </label>
                        <input asp-for="WestToFeet" value="@Model.WestToFeet" />
                        <br />
                        <p style="color:red">@ViewBag.NeedPhoto</p>
                        <label asp-for="PhotoTaken">Photo Taken?</label>
                        <select asp-for="PhotoTaken">
                            @if (Model.PhotoTaken == null)
                            {
                                <option value="" selected>Select One</option>
                                <option value="true">Yes</option>
                                <option value="false">No</option>
                            }
                            else if (Model.PhotoTaken == true)
                            {
                                <option value="">Select One</option>
                                <option value="true" selected>Yes</option>
                                <option value="false">No</option>
                            }
                            else
                            {
                                <option value="">Select One</option>
                                <option value="true">Yes</option>
                                <option value="false" selected>No</option>
                            }
                        </select>
                        <br />

                        <label asp-for="Goods">Burial Goods? </label>
                        <select asp-for="Goods">
                            @if (Model.Goods == null)
                            {
                                <option value="" selected>Select One</option>
                                <option value="true">Yes</option>
                                <option value="false">No</option>
                            }
                            else if (Model.Goods == true)
                            {
                                <option value="">Select One</option>
                                <option value="true" selected>Yes</option>
                                <option value="false">No</option>
                            }
                            else
                            {
                                <option value="">Select One</option>
                                <option value="true">Yes</option>
                                <option value="false" selected>No</option>
                            }
