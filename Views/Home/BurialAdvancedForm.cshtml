                        <label asp-for="SkullAtMagazine">Skull At Magazine: </label><br />
                        <label>&nbsp;&nbsp;&nbsp;&nbsp;</label>
                        @if (Model.SkullAtMagazine == null)
                        {
                            <label for="true">Yes</label>
                            <input type="radio" asp-for="SkullAtMagazine" value="true" id="true" />
                            <label>&nbsp;&nbsp;&nbsp;&nbsp;</label>
                            <label for="false">No</label>
                            <input type="radio" asp-for="SkullAtMagazine" value="false" id="false" />
                            <br />
                        }
                        else if (Model.SkullAtMagazine == true)
                        {
                            <label for="true">Yes</label>
                            <input type="radio" asp-for="SkullAtMagazine" value="true" id="true" checked />
                            <label>&nbsp;&nbsp;&nbsp;&nbsp;</label>
                            <label for="false">No</label>
                            <input type="radio" asp-for="SkullAtMagazine" value="false" id="false" />
                            <br />
                        }
                        else
                        {
                            <label for="true">Yes</label>
                            <input type="radio" asp-for="SkullAtMagazine" value="true" id="true" />
                            <label>&nbsp;&nbsp;&nbsp;&nbsp;</label>
                            <label for="false">No</label>
                            <input type="radio" asp-for="SkullAtMagazine" value="false" id="false" checked />
